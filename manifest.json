{
    "manifest_version": 2,
    "name": "VirusTotal Search",
    "version": "1",
    "description":"Quickly search things through Virus Total",
    "permissions":["contextMenus","tabs"],
    "background": {
        "scripts": ["background.js"]
    },
    "browser_action": {
        "default_icon": "icon.png"
    },
    "icons":{
        "16" : "icon16.png",
        "48" : "icon48.png",
        "128" : "icon128.png"
    }
}